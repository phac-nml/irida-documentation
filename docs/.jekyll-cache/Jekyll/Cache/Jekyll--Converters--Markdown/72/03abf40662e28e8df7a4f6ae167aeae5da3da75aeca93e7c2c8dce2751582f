      <span class="nt">&quot;remoteInputDataId&quot;</span> <span class="p">:</span> <span class="s2">&quot;33b43b4e7093c91f&quot;</span><span class="p">,</span>
      <span class="nt">&quot;remoteWorkflowId&quot;</span> <span class="p">:</span> <span class="s2">&quot;33b43b4e7093c91f&quot;</span><span class="p">,</span>
      <span class="nt">&quot;inputParameters&quot;</span> <span class="p">:</span> <span class="p">{</span>
        <span class="nt">&quot;assembly-contig-min-length-coverage-calculation&quot;</span> <span class="p">:</span> <span class="s2">&quot;5000&quot;</span><span class="p">,</span>
        <span class="nt">&quot;assembly-contig-min-length&quot;</span> <span class="p">:</span> <span class="s2">&quot;500&quot;</span><span class="p">,</span>
        <span class="nt">&quot;read-merge-min-overlap&quot;</span> <span class="p">:</span> <span class="s2">&quot;20&quot;</span><span class="p">,</span>
        <span class="nt">&quot;assembly-contig-min-coverage-ratio&quot;</span> <span class="p">:</span> <span class="s2">&quot;0.33&quot;</span><span class="p">,</span>
        <span class="nt">&quot;read-merge-max-overlap&quot;</span> <span class="p">:</span> <span class="s2">&quot;300&quot;</span><span class="p">,</span>
        <span class="nt">&quot;assembly-contig-min-repeat-coverage-ratio&quot;</span> <span class="p">:</span> <span class="s2">&quot;1.75&quot;</span>
      <span class="p">},</span>
      <span class="nt">&quot;createdDate&quot;</span> <span class="p">:</span> <span class="mi">1501260023000</span><span class="p">,</span>
      <span class="nt">&quot;modifiedDate&quot;</span> <span class="p">:</span> <span class="mi">1501260369000</span><span class="p">,</span>
      <span class="nt">&quot;analysisState&quot;</span> <span class="p">:</span> <span class="s2">&quot;COMPLETED&quot;</span><span class="p">,</span>
      <span class="nt">&quot;analysisCleanedState&quot;</span> <span class="p">:</span> <span class="s2">&quot;NOT_CLEANED&quot;</span><span class="p">,</span>
      <span class="nt">&quot;analysisDescription&quot;</span> <span class="p">:</span> <span class="s2">&quot;&quot;</span><span class="p">,</span>
      <span class="nt">&quot;label&quot;</span> <span class="p">:</span> <span class="s2">&quot;SISTRTyping_20170728_AE014613-699860&quot;</span><span class="p">,</span>
      <span class="nt">&quot;links&quot;</span> <span class="p">:</span> <span class="p">[</span> <span class="p">{</span>
        <span class="nt">&quot;rel&quot;</span> <span class="p">:</span> <span class="s2">&quot;self&quot;</span><span class="p">,</span>
        <span class="nt">&quot;href&quot;</span> <span class="p">:</span> <span class="s2">&quot;http://localhost:8080/api/analysisSubmissions/17&quot;</span>
      <span class="p">}</span> <span class="p">],</span>
      <span class="nt">&quot;identifier&quot;</span> <span class="p">:</span> <span class="s2">&quot;17&quot;</span>
    <span class="p">}</span> <span class="p">],</span>
    <span class="nt">&quot;links&quot;</span> <span class="p">:</span> <span class="p">[</span> <span class="p">{</span>
