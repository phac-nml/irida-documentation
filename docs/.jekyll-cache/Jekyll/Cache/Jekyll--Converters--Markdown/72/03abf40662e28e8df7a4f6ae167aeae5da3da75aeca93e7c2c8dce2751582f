                <span class="nt">&quot;href&quot;</span><span class="p">:</span> <span class="s2">&quot;http://localhost:8080/api/analysisSubmissions/analysisType/assembly&quot;</span><span class="p">,</span>
                <span class="nt">&quot;rel&quot;</span><span class="p">:</span> <span class="s2">&quot;analysisSubmissions/assembly&quot;</span>
            <span class="p">}</span>
        <span class="p">],</span>
        <span class="nt">&quot;resources&quot;</span><span class="p">:</span> <span class="p">[</span>
            <span class="p">{</span>
                <span class="nt">&quot;analysisCleanedState&quot;</span><span class="p">:</span> <span class="s2">&quot;NOT_CLEANED&quot;</span><span class="p">,</span>
